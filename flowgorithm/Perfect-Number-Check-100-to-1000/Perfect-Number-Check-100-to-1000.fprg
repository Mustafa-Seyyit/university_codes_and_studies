    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="x, toplam, i" type="Integer" array="False" size=""/>
            <assign variable="x" expression="1"/>
            <assign variable="toplam" expression="0"/>
            <for variable="i" start="100" end="1000" direction="inc" step="1">
                <while expression="i&gt;x">
                    <if expression="i%x==0">
                        <then>
                            <assign variable="toplam" expression="toplam+x"/>
                        </then>
                        <else/>
                    </if>
                    <assign variable="x" expression="x+1"/>
                </while>
                <if expression="i==toplam">
                    <then>
                        <output expression="i" newline="True"/>
                    </then>
                    <else/>
                </if>
                <assign variable="x" expression="1"/>
                <assign variable="toplam" expression="0"/>
            </for>
        </body>
    </function>
